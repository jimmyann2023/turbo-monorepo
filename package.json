{
  "name": "with-changesets",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "clean": "turbo clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=docs^... && changeset publish",
    "format": "vsh lint --format",
    "lint": "vsh lint"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@repo/vsh": "workspace:*",
    "@types/node": "^22.10.1",
    "rimraf": "^6.0.1",
    "turbo": "^2.3.3",
    "typescript": "5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "vue": "catalog:"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  }
}
